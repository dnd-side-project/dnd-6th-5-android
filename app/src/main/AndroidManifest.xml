<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fork.spoonfeed">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SpoonFeedApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpoonFeed">
      <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="oauth"
                    android:scheme="${kakaoKey}" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.ui.login.KakaoLogInActivity"
            android:exported="true">
         <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
         </activity>
       <activity
            android:name=".presentation.ui.policy.view.filter.tooltip.NetWorthTooltipActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.ui.policy.view.filter.tooltip.MedianIncomeTooltipActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.ui.policy.view.filter.PolicyFilterActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.ui.mypage.view.InterastedPolicyActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.ui.policylist.view.PolicyListActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true"/>
       <activity
            android:name=".presentation.ui.policylist.view.DetailInfoActivity"
            android:exported="false" />
    </application>

</manifest>